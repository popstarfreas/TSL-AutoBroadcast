{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "target": "es2020",
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "build",
        "strictNullChecks": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "terrariaserver-lite/*": [
                "TerrariaServer-Lite/app/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}
